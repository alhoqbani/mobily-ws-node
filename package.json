{
  "name": "mobily-ws",
  "version": "1.0.0",
  "description": "Node.js client to communicate with mobily.ws API",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "nyc mocha",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha test/**/*.spec.ts - - -R spec",
    "posttest": "nyc report --reporter=html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alhoqbani/mobily-ws-node.git"
  },
  "keywords": [
    "sms",
    "mobily",
    "mobily.ws"
  ],
  "author": "Hamoud Alhoqbani",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alhoqbani/mobily-ws-node/issues"
  },
  "homepage": "https://github.com/alhoqbani/mobily-ws-node#readme",
  "devDependencies": {
    "@types/chai": "^4.1.5",
    "@types/mocha": "^5.2.5",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "reporter": [
      "html"
    ],
    "all": true
  }
}
